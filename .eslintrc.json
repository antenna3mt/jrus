{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules":{
        "no-console": ["warn", { "allow": ["warn", "error"] }],
        "class-methods-use-this":["off"],
        "max-len": ["warn", 255],
        "no-return-assign": "warn",
        "consistent-return": "off",
        "no-shadow":"warn",
        "import/prefer-default-export": "off",
        "no-await-in-loop": "off"
    }
}